loadstring(game:HttpGet("https://raw.githubusercontent.com/Freozzy/AntiHackDelete/main/Skywars", true))()
wait(1)
game.workspace.Borders.InvisibleBorder:remove()
wait(0.1)
game.workspace.Lobby.KillPlates:remove()
wait(0.1)
local BuildPart = Instance.new("Part",game.Workspace)
BuildPart.Size = Vector3.new(500, 1, 500)
BuildPart.Position = Vector3.new(25,160,-8)
BuildPart.Anchored = true
BuildPart.Name = "AntiFall"
BuildPart.BrickColor = BrickColor.new("Bright bluish green")
BuildPart.Material = Enum.Material.SmoothPlastic
wait(0.1)
local Brick = game.Workspace.AntiFall --Change To Brick
local speed = 1 --Change to speed you want
while true do
	for i = 0,1,0.001*speed do
		Brick.Color = Color3.fromHSV(i,1,1) --creates a color using i
		wait()
	end
end
